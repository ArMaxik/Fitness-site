<h2><%= t :Login %></h2>

<%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", label: t(:Email) %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "current-password", label: t(:Password_L) %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me, label: t(:Remember) %>
    </div>
  <% end -%>
  <br>
  <div class="actions">
    <%= f.submit t(:Login) %>
  </div>
<% end %>


<%= render "devise/shared/links" %>
