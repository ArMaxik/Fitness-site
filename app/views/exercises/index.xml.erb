<?xml version="1.0" encoding="UTF-8" ?>
<exercises>
  <% @exercises.each do |exer| %>
    <exercise id="<%= exer.id %>" user_id="<%= exer.user_id %>">
      <id><%= exer.id %></id>
      <title><%= exer.title %></title>
      <description><%= exer.description%></description>
      <rating><%= exer.rating %></rating>
      <user_name><%= exer.user.username %></user_name>
      <user_vote><%= VoteTable.find_by(user_id:current_user.id, exercise_id: exer.id ).vote %></user_vote>
    </exercise>
  <% end %>
</exercises>